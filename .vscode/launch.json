    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch on iOS Emulator",
            "type": "dart",
            "request": "launch",
            "platform": "ios",
            "appRoot": "${workspaceRoot}",
            "sourceMaps": true,
            "watch": true,
            "emulator": true,
        },
        {
            "name": "flutter_app_qiita",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_app_qiita (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_app_qiita (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}